<%= simple_form_for [current_user, @resource] do |f| %>
  <%= f.input :title %>
  <%= f.input :url, placeholder: "Optional" %>
  <%= f.input :body, placeholder: "Optional" %>
  <%= f.input :upload, as: :file %>
  <%= f.button :submit %>
<% end %>
<%= link_to "Back to Profile", user_path(current_user), class: "button" %>
