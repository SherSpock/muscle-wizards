<%= line_chart [
  {
    name: "Protein",
    data: @macros.map { |macro| [macro.created_at, macro.protein] }
  },
  {
    name: "Carbs",
    data: @macros.map { |macro| [macro.created_at, macro.carbs] }
  },
  {
    name: "Fat",
    data: @macros.map { |macro| [macro.created_at, macro.fat] }
  },
  {
    name: "Target Protein",
    data: @target_macros.map { |macro| [macro.created_at, macro.protein] }
  },
  {
    name: "Target Carbs",
    data: @target_macros.map { |macro| [macro.created_at, macro.carbs] }
  },
  {
    name: "Target Fat",
    data: @target_macros.map { |macro| [macro.created_at, macro.fat] }
  }
  ],
  ytitle: "Macro Quantity (g)" %>
