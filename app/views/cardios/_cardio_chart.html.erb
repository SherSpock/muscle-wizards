<%= line_chart [
  {
    name: "Cardio",
    data: @cardios.map { |cardio| [cardio.created_at.strftime('%D'), cardio.duration] }
  },
  {
    name: "Target Cardio",
    data: @target_cardios.map { |cardio| [cardio.created_at.strftime('%D'), cardio.duration] }
  }
  ],
  ytitle: "Duration (min)", min: @minmax[0], max: @minmax[1] %>
